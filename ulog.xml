<CodeSnippet>
    <Header>
        <Shortcut>ulog</Shortcut>
    </Header>
    <Snippet>
        <Code Language="CPP">
            <![CDATA[UE_LOG($LogType$, $Severity$, $Data$);]]>
        </Code>
        <Declarations>
            <Literal>
                <ID>LogType</ID>
                <ToolTip>The category of Log (Temp, Physics, Level, etc)</ToolTip>
                <Default>LogTemp</Default>
            </Literal>
            <Literal>
                <ID>Severity</ID>
                <ToolTip>Display, Warning, Error, Fatal, etc...</ToolTip>
                <Default>Display</Default>
            </Literal>
            <Literal>
                <ID>Data</ID>
                <Default>TEXT("")</Default>
            </Literal>
        </Declarations>
    </Snippet>
</CodeSnippet>